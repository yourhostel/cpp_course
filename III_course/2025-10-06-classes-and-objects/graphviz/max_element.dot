digraph max_element {
    rankdir=TB;
    node [
    shape=box,
    style="solid,bold",
    fontname="DejaVu Sans",
    fontsize=11,
    margin="0.15,0.1",
    fixedsize=false
    ];

    start [shape=oval, label="Початок"];
    init  [shape=rectangle, label="maxVal = -inf"];
    loop1 [shape=diamond, label="Є наступний рядок?"];
    loop2 [shape=diamond, label="Є елемент у рядку?"];
    check [shape=diamond, label="el > maxVal ?"];
    assign [shape=rectangle, label="maxVal = el"];
    endok [shape=oval, label="Вивід maxVal"];

    start -> init -> loop1;
    loop1 -> loop2 [label="так"];
    loop2 -> check [label="так"];
    check -> assign [label="так"];
    assign -> loop2;
    check -> loop2 [label="ні"];
    loop2 -> loop1 [label="ні"];
    loop1 -> endok [label="ні"];
}
